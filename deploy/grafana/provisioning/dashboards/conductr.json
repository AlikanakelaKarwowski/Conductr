{
    "uid": "conductr",
    "title": "Conductr Overview",
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "5s",
    "time": {
        "from": "now-1h",
        "to": "now"
    },
    "panels": [
        {
            "type": "timeseries",
            "title": "Interactions received (rate)",
            "targets": [
                {
                    "datasource": {
                        "type": "prometheus",
                        "uid": "Prometheus"
                    },
                    "expr": "rate(discord_interactions_total[1m])",
                    "legendFormat": "interactions/min"
                }
            ],
            "gridPos": {
                "h": 8,
                "w": 24,
                "x": 0,
                "y": 0
            }
        },
        {
            "type": "timeseries",
            "title": "Gateway latency (s)",
            "targets": [
                {
                    "datasource": {
                        "type": "prometheus",
                        "uid": "Prometheus"
                    },
                    "expr": "gateway_latency_seconds",
                    "legendFormat": "latency"
                }
            ],
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 8
            }
        },
        {
            "type": "timeseries",
            "title": "Web page hits (rate)",
            "targets": [
                {
                    "datasource": {
                        "type": "prometheus",
                        "uid": "Prometheus"
                    },
                    "expr": "rate(web_page_hits_total[1m])",
                    "legendFormat": "hits/min"
                }
            ],
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 8
            }
        }
    ]
}
